{
    "mongo": {
        "args": [
            "--config",
            "/usr/local/etc/mongod.conf"
        ],
        "color": "cyan",
        "command": "mongod"
    },
    "redis": {
        "color": "yellow",
        "command": "redis-server"
    },
    "server": {
        "args": [
            "server.js",
            "--port",
            "8006"
        ],
        "color": "magenta",
        "command": "node_modules/.bin/node-dev",
        "resolvePath": true,
        "env": {}
    }
}
